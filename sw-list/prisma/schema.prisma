generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "mysql"
  url       = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  slackId   String   @unique
  username  String
  avatar    String?
  isActive  Boolean  @default(true)
  role      String   @default("trainee")

  @@map("users")
}
